    <elem name="document" />
  </group>
  
  <group uri="http://exslt.org/dates-and-times">  
    <func name="add" />
    <func name="add-duration" />
    <func name="date" />
    <func name="date-time" />
    <func name="day-abbreviation" />
    <func name="day-in-month" />
    <func name="day-in-week" />
    <func name="day-in-year" />
    <func name="day-name" />
    <func name="day-of-week-in-month" />
    <func name="difference" />
    <func name="duration" />
    <func name="format-date" />
    <func name="hour-in-day" />
    <func name="leap-year" />
    <func name="minute-in-hour" />
    <func name="month-abbreviation" />
    <func name="month-in-year" />
    <func name="month-name" />
    <func name="parse-date" />
    <func name="second-in-minute" />
    <func name="seconds" />
    <func name="sum" />
    <func name="time" />
    <func name="week-in-month" />
    <func name="week-in-year" />
    <func name="year" />
    <elem name="date-format" />
  </group>
 
  <group uri="http://exslt.org/dynamic">
    <func name="closure" />
    <func name="evaluate" />
    <func name="map" />
    <func name="max" />
    <func name="min" />
    <func name="sum" />
  </group>

  <group uri="http://exslt.org/functions">
    <elem name="function" />
    <elem name="result" />
    <elem name="script" />
  </group>
  
  <group uri="http://exslt.org/math">
    <func name="abs" />
    <func name="acos" />
    <func name="asin" />
    <func name="atan" />
    <func name="atan2" />
    <func name="constant" />
    <func name="cos" />
    <func name="exp" />
    <func name="highest" />
    <func name="log" />
    <func name="lowest" />
    <func name="max" />
    <func name="min" />
    <func name="power" />
    <func name="random" />
    <func name="sin" />
    <func name="sqrt" />
    <func name="tan" />
  </group>
  
  <group uri="http://exslt.org/random">
